{
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "json.schemas": [
        {
            "fileMatch": [
                "manifest.json"
            ],
            "url": "/manifest.schema.json"
        }
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "cSpell.words": [
        "iife",
        "nacl",
        "newtab",
        "omnibox",
        "tsconfigs",
        "usecase",
        "usecases",
        "vite"
    ],
    "mochaExplorer": {
        "files": "test/**/*.ts",
        "require": [
            "ts-node/register",
            "tsconfig-paths/register"
        ],
        "env": {
            "TS_NODE_COMPILER_OPTIONS": "{\"module\": \"commonjs\"}"
        }
    }
}
